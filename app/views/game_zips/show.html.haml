%p#notice
  = notice

%p
  %b Zip:
  = @game_zip.zip

%p
  %b Cultural Organizations:
  %ul
  - if @game_zip.cultural_organizations.length == 0
    None
  - else 
    - @game_zip.cultural_organizations.each do|org|
      %li
        = org.organization_name
        = org.discipline_code

%p
  %b Community Health Centers:
  %ul
  - if @game_zip.community_health_centers.length == 0
    %li
      Sorry
  - else
    -@game_zip.community_health_centers.each do |center|
      %li
        = center.name_of_center

/
  %p
    %b Complaints:
    %ul
    - if @game_zip.complaints.length == 0
      %li
        Yay, everyone is happy!
    - else
      -@game_zip.complaints.each do |complaint|
        %li
          = complaint.complaint_type
          %br
          = complaint.descriptor
          %br
          = complaint.incident_address
          %br
%p
  %b NYC StartUp Jobs:
  %ul
  -if @game_zip.start_up_jobs.length == 0
    Looks like there are no NYC start up companies for you here.
  -else
    -@game_zip.start_up_jobs.each do |job|
      %li
      = job.company_name
      = job.address
      = job.hiring


%p 
  %b Restaurants:
  %ul
  -if @game_zip.restaurants.length == 0
    Sucks for you, you're in a food desert!
  -else
    -@game_zip.restaurants.each do |place|
      %li
      = place.name.capitalize + ","
      = place.street_address + ","
      = place.current_grade + ","
      = place.violation

%p 
  %b Youth Programs:
  %ul
  -if @game_zip.youth_programs.length == 0
    You don't have any youth programs in your zip code, hope you don't have any rowdy young'uns!
  -else
    %li
    -game_zip.youth_programs.each do |element|
      = element.address
  
      
    




